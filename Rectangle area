// This little project is meant to help learn arrays and nested for loops but I'm missing something to keep it from constantly printing.

int length [5] = {4, 8, 12, 16, 20};
int width [5] = {2, 4, 6, 8, 10};

void setup()
{
Serial.begin (9600);      
}
void loop()
{
 for (int l = 0; l < 5; l ++){ // this line and the next determine the elements of the arrays
    for (int w = 0; w < 5; w ++){
      int area = length [l] * width [w]; // defines area
Serial.println (area);
    }
 }
}
